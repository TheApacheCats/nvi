all: ${HDRS} nvi @tknvi@

NVIALL=	${CLOBJS} ${VIOBJS} @cobjs@ @dbobjs@ @reobjs@ @LIBOBJS@
nvi: ${NVIALL}
	${CC} -o $@ ${NVIALL} @LIBS@

TKALL=	${TKOBJS} ${VIOBJS} @dbobjs@ @reobjs@ @LIBOBJS@
tknvi: ${TKALL}
	${CC} -o $@ ${TKALL} -L/usr/X11/lib -ltk -ltcl -lm -lX11

clean:
	rm -f nex nvi tknvi @LIBOBJS@ \
	    ${COBJS} ${CLOBJS} ${DBOBJS} ${REOBJS} ${TKOBJS} ${VIOBJS}

hdrs: ${HDRS}

distclean: clean
	rm -f Makefile port.h
	rm -f curses_include_path curses_library_path
	rm -f config.cache config.h config.log config.status

maintainer-clean: distclean
	rm -f ${HDRS}
	rm -f configure config.h.in

distrib: maintainer-clean
	autoheader
	chmod 444 config.h.in
	autoconf
	chmod 555 configure
	make -f Makefile.in hdrs
	
install:

tags:
	ctags ../*/*.h ../*/*.[cys]

# Header sources.
cl_extern.h:
